[
  {
    "name": "Message Queue",
    "id": "com.camunda.example.MessageTypeStartActiveMQEvent",
    "appliesTo": [
      "bpmn:StartEvent",
      "bpmn:EndEvent"
    ],
    "properties": [
      {
        "label": "Component Type",
        "type": "Hidden",
        "value": "activemq:",
        "binding": {
          "type": "camunda:property",
          "name": "COMPONENT"
        }
      },
      {
        "label": "Partner ID",
        "type": "Dropdown",
        "description": "Set Partner ID",
        "choices":[{"name": "SELF" ,"value": "SELF"}],
        "value":"SELF",
        "binding": {
          "type": "camunda:property",
          "name": "PARTNER_ID"
        }
      },
      {
        "label": "Endpoint ID",
        "type": "Dropdown",
        "description": "Set Endpoint ID",
        "choices":[{"name": "HULK-TDH" ,"value": "HULK-TDH"}],
        "value":"HULK-TDH",
        "binding": {
          "type": "camunda:property",
          "name": "ENDPOINT_ID"
        }
      },
      {
        "label": "Message ID",
        "type": "Dropdown",
        "description": "Set Message ID",
        "choices":[{"name": "appconfig" ,"value": "appconfig"},{"name": "moduleconfig" ,"value": "moduleconfig"}],
        "value":"appconfig",
        "binding": {
          "type": "camunda:property",
          "name": "MESSAGE_ID"
        }
      },
      {
        "label": "Message Name",
        "type": "Text",
        "description": "Set Message Name",
        "value":"{{COMPONENT}}:INBOUND:{{PARTNER_ID}}:{{ENDPOINT_ID}}:{{MESSAGE_ID}}",
        "binding": {
          "type": "camunda:property",
          "name": "MESSAGE_NAME"
        }
      }
    ]
  }
]
